{
  "target": "bmv2",
  "p4info": "build/basic.p4.p4info.txt",
  "bmv2_json": "build/basic.json",
  "table_entries": [
    {
      "table": "MyIngress.ipv4_lpm",
      "default_action": true,
      "action_name": "MyIngress.drop",
      "action_params": { }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4_outer.dstAddr": ["10.0.1.1", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "00:00:00:00:01:01",
        "port": 1
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4_outer.dstAddr": ["10.0.1.2", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "00:00:00:00:01:02",
        "port": 2
      }
    },
    {
      "table": "MyIngress.ipv4_lpm",
      "match": {
        "hdr.ipv4_outer.dstAddr": ["10.0.1.3", 32]
      },
      "action_name": "MyIngress.ipv4_forward",
      "action_params": {
        "dstAddr": "00:00:00:00:01:03",
        "port": 3
      }
    },
    {
	"table": "MyIngress.gtp_exact",
	"default_action": true,
	"action_name": "MyIngress.ipv4_forward",
	"action_params": {
	    "dstAddr": "00:00:00:00:01:02",
	    "port": 2
	}
    },
    {
	"table": "MyIngress.gtp_exact",
	"match": {
	    "hdr.ipv4_inner.srcAddr": ["45.45.0.20", 32]
	},
	"action_name": "MyIngress.gtp_forward",
	"action_params": {
	    "dst_ip": "10.0.1.3",
	    "dstAddr": "00:00:00:00:01:03",
	    "port": 3
	}
    }      

  ]
}
